datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id    Int    @id
  name  String
  email String
}

model Cinema {
  id String @id
  title String
  description String
  genres String
  type String
  images String
}

model Book {
  id String @id
  title String
  author String
  description String
  genre String
}
